<h1>Jokes</h1>

<table>
  <tr>
    <th>Setup</th>
    <th>Punchline</th>
    <th>Author</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jokes.each do |joke| %>
  <tr>
    <td><%= joke.setup %></td>
    <td><%= joke.punchline %></td>
    <td><%= joke.author %></td>
    <td><%= link_to 'Show', joke %></td>
    <td><%= link_to 'Edit', edit_joke_path(joke) %></td>
    <td><%= link_to 'Destroy', joke, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Joke', new_joke_path %>
